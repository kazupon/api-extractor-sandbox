{
  "name": "api-extractor-sandbox",
  "version": "1.0.0",
  "description": "",
  "typings": "./dist/api-extractor-sandbox.d",
  "scripts": {
    "build": "tsc -p .",
    "generate": "yarn build && api-extractor run --local --verbose",
    "docs": "api-documenter markdown -i ./temp -o ./docs",
    "docs:gen": "api-documenter generate -i ./temp",
    "clean": "rm -rf ./etc/*.md ./temp ./lib ./dist ./docs/*.md ./generate/*.md"
  },
